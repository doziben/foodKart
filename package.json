{
  "name": "foodkart",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tw": "npx tailwindcss -i ./src/index.css -o ./src/output.css --watch",
    "watch": "chokidar \"./src/**/*.jsx\" -c \"npm run tw\"",
    "main": "concurrently \"npm run watch\" \"npm run dev\""
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.2.2",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.3",
    "vite": "^2.9.9"
  }
}
